{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TestDeploySchema",
  "description": "A simple schema for testing deployment, UID generation, and various property types.",
  "type": "object",
  "properties": {
    "schemaId": {
      "type": "integer",
      "title": "Schema ID",
      "description": "A unique identifier for this schema instance."
    },
    "msg9": {
      "type": "string",
      "title": "Message",
      "description": "A test message."
    },
    "isActive": {
      "type": "boolean",
      "title": "Is Active",
      "description": "A boolean flag."
    },
    "tags": {
      "type": "array",
      "title": "Tags",
      "items": {
        "type": "string"
      },
      "description": "An array of simple string tags."
    },
    "scores": {
      "type": "array",
      "title": "Scores",
      "items": {
        "type": "integer"
      },
      "description": "An array of integer scores."
    },
    "optionalDescription": {
      "type": ["string", "null"],
      "title": "Optional Description",
      "description": "An optional string field that can be null."
    },
    "statusFlags": {
      "type": "array",
      "title": "Status Flags",
      "items": {
        "type": "boolean"
      },
      "description": "An array of boolean flags."
    }
  },
  "required": [
    "schemaId",
    "msg9",
    "tags"
  ]
} 
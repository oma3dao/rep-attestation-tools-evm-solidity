{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Arrays",
  "description": "Schema with array types",
  "type": "object",
  "properties": {
    "stringArray": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "An array of strings"
    },
    "integerArray": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "description": "An array of integers"
    },
    "booleanArray": {
      "type": "array",
      "items": {
        "type": "boolean"
      },
      "description": "An array of booleans"
    },
    "nullableArray": {
      "type": ["array", "null"],
      "items": {
        "type": "string"
      },
      "description": "An optional array of strings"
    }
  },
  "required": ["stringArray", "integerArray"]
}